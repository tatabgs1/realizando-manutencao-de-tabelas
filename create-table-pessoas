CREATE TABLE pessoas(
id INT NOT NULL PRIMARY KEY AUTOINCREMENT,
nome VARCHAR(30) NOT NULL,
nascimento DATE
)

INSERT INTO pessoas (nome,nascimento) VALUES ('Tassiana', '1992-01-22')
INSERT INTO pessoas (nome,nascimento) VALUES ('Gisele', '1990-04-15')
INSERT INTO pessoas (nome,nascimento) VALUES ('Juliana', '1998-02-10')

UPDATE pessoas SET nom='Tassiana Borges' WHERE id='1'

DELETE FROM pessoas WHERE id=4;

SELECT FROM pessoas ORDER BY nome;
SELECT FROM pessoas ORDER BY nome DESC;


